armors:

## Sentinels

  - type: STR_SENTINELPD_ARMOR_MISSILE_UC
  - type: STR_SENTINELPD_ARMOR_UC # LASCAN

## Penitents

  - type: STR_PENAL_ARMOR_UC

## Troopers

  - delete: STR_GUARDPD_ARMOR_UC
  - delete: STR_GUARDPDMEDIC_ARMOR_UC

  # get rid of the stats modifiers
  # everything has to be done in this single file
  - delete: STR_ENFORCER_ARMOR_UC
  - delete: STR_ENFORCER_MEDIC_ARMOR_UC
  - delete: STR_ENFORCER_SHIELD_ARMOR_UC

  - &STR_BASIC_ENFORCER_ARMOR
    type: STR_ENFORCER_ARMOR_UC
    storeItem: STR_NONE
    visibilityAtDay: 40
    visibilityAtDark: 20
    frontArmor: 50
    sideArmor: 45
    rearArmor: 30
    underArmor: 40
    damageModifier: #GUARD ARMOR
      - 1.0 #none
      - 1.0 #AP
      - 1.2 #FLAMES
      - 1.0 #HE
      - 0.8 #LASCANON
      - 1.0 #PLASMA
      - 1.0 #STUN
      - 1.1 #MELEE
      - 1.0 #ACID
      - 0.0 #SMOKE
      - 1.0 #IMPACT
      - 1.0 #MELTA
    units:
      - STR_GUARDPD
    forcedTorso: 1
    spriteFaceGroup: 6
    spriteFaceColor: [96, 96, 96, 96, 160, 160, 163, 163, 96, 96, 96, 96, 96, 96, 96, 96, 160, 160, 163, 163] #M0 F0 M1 F1 M2 F2 M3 F3 M4 F4 M5 F5 M6 F6 M7 F7 M8 F8 M9 F9
    spriteHairGroup: 9
    spriteHairColor: [144, 144, 164, 164, 245, 245, 166, 166, 96, 164, 96, 96, 144, 144, 164, 164, 245, 245, 166, 166] #M0 F0 M1 F1 M2 F2 M3 F3 M4 F4 M5 F5 M6 F6 M7 F7 M8 F8 M9 F9
    corpseBattle:
      - STR_ENFORCER_CORPSE
    loftempsSet: [ 3 ]
    drawingRoutine: 0
    spriteSheet: ENFORCER.PCK
    customArmorPreviewIndex: {mod: 40k, index: 47}

  - type: STR_ENFORCER_MEDIC_ARMOR_UC
    refNode: *STR_BASIC_ENFORCER_ARMOR
    spriteSheet: ENFORCERM.PCK
    customArmorPreviewIndex: {mod: 40k, index: 45}
    storeItem: STR_ENFORCER_MEDIC_ARMOR
    weight: 5
    specialWeapon: STR_GUARD_MEDI_KIT
    builtInWeapons:
      - INV_NULL_MEDI_KIT

  - type: STR_ENFORCER_SHIELD_ARMOR_UC
    refNode: *STR_BASIC_ENFORCER_ARMOR
    storeItem: STR_RIOT_SHIELD
    builtInWeapons:
      - AUX_SHIELDPD
    corpseBattle:
      - STR_ENFORCER_SHIELD_CORPSE

  - type: STR_ENFORCER_HEAVY_ARMOR_UC
    storeItem: STR_HEAVY_RIOT_SHIELD  

  - type: STR_ENFORCER_SHIELD_HEAVY_ARMOR_UC #DR10 ARBITES SUPORT
    refNode: *STR_BASIC_ENFORCER_ARMOR
    spriteSheet: ENFORCERH.PCK
    customArmorPreviewIndex: {mod: 40k, index: 49}
    storeItem: STR_RIOT_SHIELD # STR_HEAVY_RIOT_SHIELD
    drawingRoutine: 0
    loftempsSet: [ 3 ]
    builtInWeapons:
      - AUX_SHIELDPD
    corpseBattle:
      - STR_ENFORCER_HEAVY_SHIELD_CORPSE

  - type: STR_JUDGE_ARMOR_UC
    frontArmor: 60
    sideArmor: 55
    rearArmor: 40
    underArmor: 50 

  - type: STR_ENFORCER_SIGNAL_ARMOR_UC
  - type: STR_ENFORCER_SIGNALFALSE_ARMOR_UC


  - type: STR_ARBITOR_ARMOR_UC
  - type: STR_MARSHAL_ARMOR_UC


  - type: STR_CMAS_ARMOR
    customArmorPreviewIndex: 600


